{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Npgsql": "Warning",
        "Wolverine": "Warning",
        "WolverineFx": "Warning",
        "System": "Warning",
        "Microsoft": "Warning"
      }},
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Properties": {
      "Application": "AssociationRegistry - Admin Address Synchronisation Task",
      "ContainerId": "REPLACE_CONTAINERID"
    }
  },
  "PostgreSQLOptions": {
    "Host": "127.0.0.1",
    "database": "verenigingsregister",
    "username": "root",
    "password": "root"
  },
  "AddressSyncOptions": {
    "BaseUrl": "http://127.0.0.1",
    "ApiKey": "abc",
    "SlackWebhook": "http://127.0.0.1"
  }
}
